@   IN  NS  ns1.mg.asa.br.
;@   IN  MX 10    mail.mg.asa.br.
@   IN  A   10.25.2.19

ns1     IN  A   10.25.2.19
mail    IN  A   10.25.2.19
www     IN  CNAME   ns1